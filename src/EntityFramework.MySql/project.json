{
  "version": "1.0.0-*",
  "description": "MySql data store for Entity Framework.",
  "repository": {
    "type": "git",
    "url": "git://github.com/aspnet/entityframework"
  },
  "compilationOptions": {
    "warningsAsErrors": true
  },
  "dependencies": {
    "EntityFramework.Relational": "7.0.0-*",
    "MySql.Data": "6.9.8",
    "build": "0.4.6"
  },
  "compile": "..\\Shared\\*.cs",
